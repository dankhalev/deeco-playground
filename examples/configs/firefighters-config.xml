<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<visualization zoom="6" maxCPS="15" fontColor="Green">
	<additional class="cz.cuni.mff.d3s.deeco.playground.examples.firefighters.environment.TemperatureLayer" arg="simulation-logs/firemap.bin"/>
	<objects circularShape="true">
		<class-based>
			<default texture="examples/images/station-red.png"/>
		</class-based>
	</objects>
	<robots displayNumbers="true" enableRotation="true" fontColor="Blue">
		<tag-based>
			<default texture="examples/images/robot.png"/>
			<tag name="BROKEN" texture="examples/images/robot_black.png"/>
			<tag name="ROAM" texture="examples/images/robot_green.png"/>
			<tag name="FOLLOW" texture="examples/images/robot_yellow.png"/>
			<tag name="FIGHT" texture="examples/images/robot_blue.png"/>
			<tag name="RECHARGE" texture="examples/images/robot_red.png"/>
		</tag-based>
	</robots>
</visualization>
