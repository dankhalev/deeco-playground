<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<visualization zoom="8" maxCPS="15" fontColor="Green">
	<background texture="examples/images/bricks.jpg"/>
	<objects circularShape="true">
		<class-based>
			<default color="orange"/>
			<class name="com.khalev.efd.robots.competition.CommandStation" texture="examples/images/station-red.png"/>
			<class name="com.khalev.efd.robots.competition.PowerStation" texture="examples/images/station-blue.png"/>
		</class-based>
	</objects>
	<robots displayNumbers="true" displayTags="true" enableRotation="true" fontColor="green">
		<class-based>
			<default color="red"/>
			<class name="com.khalev.efd.robots.competition.ControlledRobot" texture="examples/images/robot.png"/>
			<class name="com.khalev.efd.robots.competition.AutonomousRobot" texture="examples/images/robot_sprite.png"/>
		</class-based>
	</robots>
</visualization>
