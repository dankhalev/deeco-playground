<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<simulation cycles="500" logfile="simulation-logs/firefighters.txt" sizeX="100" sizeY="100">
	<sensors>
		<sensor name="coords" processor="com.khalev.efd.robots.firefighters.environment.CoordinatesProcessor"/>
		<sensor name="energy" processor="com.khalev.efd.robots.firefighters.environment.EnergyTemperatureProcessor" arg="simulation-logs/firemap.txt"/>
	</sensors>
	<robots>
		<robot class="com.khalev.efd.robots.firefighters.leader.FirefighterLeader" posX="55.0" posY="55" angle="180"/>
		<robot class="com.khalev.efd.robots.firefighters.drone.FirefighterDrone" posX="50.0" posY="50" angle="180"/>
		<robot class="com.khalev.efd.robots.firefighters.drone.FirefighterDrone" posX="60.0" posY="50" angle="180"/>
		<robot class="com.khalev.efd.robots.firefighters.drone.FirefighterDrone" posX="50.0" posY="60" angle="180"/>
		<robot class="com.khalev.efd.robots.firefighters.drone.FirefighterDrone" posX="60.0" posY="60" angle="180"/>
	</robots>
	<ensembles>
		<ensemble class="com.khalev.efd.robots.firefighters.drone.StationEnsemble"/>
		<ensemble class="com.khalev.efd.robots.firefighters.leader.DataAggregationAndOrderDistribution"/>
		<ensemble class="com.khalev.efd.robots.firefighters.drone.ModeSwitch"/>
	</ensembles>
	<objects>
		<object class="com.khalev.efd.robots.firefighters.drone.ChargingStation" posX="55.0" posY="55" size="12.0"/>
	</objects>
</simulation>
